## 지역 테이블
create table region(
region_num bigint not null auto_increment,
region_parent varchar(15) not null,
region_child varchar(15) not null,
region_grand_parent varchar(15) not null,
nx int,
ny int,
primary key(region_num)
);

## 날씨 테이블
create table weather(
region_num bigint,
weather_per int,
weather_sky varchar(10),
weahter_high_temp int,
weather_row_temp int,
primary key(region_num)
);

